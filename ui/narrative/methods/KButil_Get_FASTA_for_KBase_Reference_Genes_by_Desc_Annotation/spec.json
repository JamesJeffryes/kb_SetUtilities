{
    "ver": "0.0.1",
    "authors": [
        "dylan"
    ],
    "contact": "help@kbase.us",
    "visible": true,
    "categories": ["active","util","comparative_genomics"],
    "widgets": {
        "input": null,
        "output": "kbaseReportView"
    },
    "parameters": [ 
        {
            "id": "annotation_string",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
		"is_output_name": false
            }
        },
        {
            "id": "substr_flag",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "0" ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                                {
                                        "value": "0",
                                        "display": "require full length match",
                                        "id": "fulllen",
                                        "ui-name": "fulllen"
                                },
                                {
                                        "value": "1",
                                        "display": "sub match OK",
                                        "id": "sub",
                                        "ui-name": "sub"
                                }
                           ]
            }
        },
        {
            "id": "seq_type",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "DNA" ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                                {
                                        "value": "DNA",
                                        "display": "DNA",
                                        "id": "DNA",
                                        "ui-name": "DNA"
                                },
                                {
                                        "value": "AA",
                                        "display": "Protein",
                                        "id": "AA",
                                        "ui-name": "AA"
                                }
                           ]
            }
        }
    ],

    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_util_dylan",
            "method": "KButil_Get_FASTA_for_KBase_Reference_Genes_by_Desc_Annotation",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "annotation_string",
                    "target_property": "annotation_string"
                },
                {
                    "input_parameter": "substr_flag",
                    "target_property": "substr_flag"
                },
                {
                    "input_parameter": "seq_type",
                    "target_property": "seq_type"
                }
            ],
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
		    "constant_value": "5",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}