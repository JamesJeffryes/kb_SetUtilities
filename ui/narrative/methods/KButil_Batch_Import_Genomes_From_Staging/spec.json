{
    "ver": "1.2.0",
    "authors": [
        "dylan"
    ],
    "contact": "http://kbase.us/contact-us",
    "visible": true,
    "categories": ["active","util","annotation","comparative_genomics"],
    "widgets": {
        "input": null,
        "output": "kbaseReportView"
    },
    "parameters": [ 
        {
            "id": "desc",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
		"is_output_name": false
            }
        },
        {
            "id": "staging_folder_path",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "/" ],
            "field_type": "text",
            "text_options": {
		"is_output_name": false
            }
        },
        {
            "id": "genome_type",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "isolate" ],
            "field_type": "dropdown",
            "dropdown_options": {
		"is_output_name": false,
                "options": [ 
		    {
			"value": "isolate",
			"display": "Isolates",
			"id": "genome_type-isolate",
			"ui_name": "genome_type-isolate"
		    },
		    {
			"value": "mag",
			"display": "Metagenome Assembled Genomes (MAG)",
			"id": "genome_type-mag",
			"ui_name": "genome_type-mag"
		    },
		    {
			"value": "sag",
			"display": "Single Amplified Genome (SAG)",
			"id": "genome_type-sag",
			"ui_name": "genome_type-sag"
		    },
		    {
			"value": "virus",
			"display": "Viruses",
			"id": "genome_type-virus",
			"ui_name": "genome_type-virus"
		    },
		    {
			"value": "plasmid",
			"display": "Plasmids",
			"id": "genome_type-plasmid",
			"ui_name": "genome_type-plasmid"
		    },
		    {
			"value": "other",
			"display": "Other",
			"id": "genome_type-other",
			"ui_name": "genome_type-other"
		    }
		]
            }
        },
        {
            "id": "output_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "Batch_Import.GenomeSet" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseSearch.GenomeSet" ],
		"is_output_name": true
            }
        },
	{
	    "id" : "source",
	    "optional" : false,
	    "advanced" : true,
	    "allow_multiple" : false,
	    "default_values" : [ "User Upload" ],
            "field_type": "text",
            "text_options": {
		"is_output_name": false
            }
        },
	{
	    "id" : "taxon_wsname",
	    "optional" : true,
	    "advanced" : true,
	    "allow_multiple" : false,
	    "default_values" : [ "" ],
	    "field_type" : "text",
	    "text_options" : {}
	},
	{
	    "id" : "taxon_reference",
	    "optional" : true,
	    "advanced" : true,
	    "allow_multiple" : false,
	    "default_values" : [ "" ],
	    "field_type" : "text",
	    "text_options" : {}
	},
	{  
	    "id": "release",
	    "optional": true,
	    "advanced": true,
	    "allow_multiple": false,
	    "default_values": [ "" ],
	    "field_type" : "text" 
	},
	{  
	    "id": "genetic_code",
	    "optional": true,
	    "advanced": true,
	    "allow_multiple": false,
	    "default_values": [ "11" ],
	    "field_type" : "text",
	    "text_options" : {
		"validate_as": "int"
	    }
	},
	{
	    "id": "generate_missing_genes",
	    "optional":true,
	    "advanced":true,
	    "allow_multiple":false,
	    "default_values":["1"],
	    "field_type" : "checkbox",
	    "checkbox_options":{
		"checked_value": 1,
		"unchecked_value": 0
	    }
	}

    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_SetUtilities",
            "method": "KButil_Batch_Import_Genomes_From_Staging",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "desc",
                    "target_property": "desc"
                },
                {
                    "input_parameter": "staging_folder_path",
                    "target_property": "staging_folder_path"
                },
                {
                    "input_parameter": "genome_type",
                    "target_property": "genome_type"
                },
                {
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                },
		{
		    "input_parameter": "source",
		    "target_property": "source"
		},
		{
		    "input_parameter": "taxon_wsname",
		    "target_property": "taxon_wsname"
		},
		{
		    "input_parameter": "taxon_reference",
		    "target_property": "taxon_reference"
		},
		{
		    "input_parameter": "release",
		    "target_property": "release"
		},
		{
		    "input_parameter": "genetic_code",
		    "target_property": "genetic_code"
		},
		{
		    "input_parameter": "generate_missing_genes",
		    "target_property": "generate_missing_genes"
		}
            ],
            "output_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
		{
		    "constant_value": "KBaseGenomes.Genome",
		    "target_property": "type"
		},
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
		    "constant_value": "5",
                    "target_property": "report_window_line_height"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
